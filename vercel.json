{
  "version": 2,
  "installCommand": "cd api && npm install",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node@3.0.7",
      "config": {
        "includeFiles": [
          "api/**/**"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/api", "dest": "/api/index.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" }
  ]
}
